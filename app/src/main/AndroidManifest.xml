<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technuoma.emartindia">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name="com.technuoma.emartindia.Bean"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name="com.technuoma.emartindia.Address"/>
        <activity android:name="com.technuoma.emartindia.Web" />
        <activity
            android:name="com.technuoma.emartindia.OrderDetails"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.OTP"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Location"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Search"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Orders"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Checkout"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Cart"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.SingleProduct"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Products"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.SubCat"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Category"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Login"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.technuoma.emartindia.Spalsh"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.technuoma.emartindia.MainActivity" />
        <activity android:name="com.technuoma.emartindia.WebViewActivity" />

        <service
            android:name="com.technuoma.emartindia.MyFirebaseMessagingService"
            android:permission="">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <meta-data
            android:name="com.google.firebase.messaging.default_notification_icon"
            android:resource="@drawable/back" />
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_color"
            android:resource="@color/colorAccent" />
        <!--
 Set custom default icon. This is used when no icon is set for incoming notification messages.
     See README(https://goo.gl/l4GJaQ) for more.
        -->
        <meta-data
            android:name="com.google.firebase.messaging.default_notification_channel_id"
            android:value="@string/default_notification_channel_id" />
    </application>

</manifest>